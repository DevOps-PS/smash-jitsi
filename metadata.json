  "summary": "Installs, configures and manages a Jitsi Meet instance",
  "license": "GPL-3.0",
  "source": "https://gitlab.com/shared-puppet-modules-group/jitsimeet",
  "project_page": "https://gitlab.com/shared-puppet-modules-group/jitsimeet",
